{
    "routes": [
        {
            "src": "/chat",
            "dest": "/Chat"
        },
        {
            "src": "/[^.]+",
            "dest": "/",
            "status": 200
        }
    ],
    "github": {
        "silent": true
    },
    "rewrites": [
        {
            "source": "(.*)",
            "destination": "/index.html"
        }
    ]
}